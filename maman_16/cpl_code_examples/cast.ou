/* should print 3  8   16.0   17.0  18.0 (or 19.0 if RTOI rounds up) */
a, b: float;
i, j: int;

{
   a = 3.2;
   j = 8;
   
   i = (int)a;
   output(i);
   
   i = (int) j;
   output(i);
   
   i = 16;
   a = (float)i;
   output(a);
   
   b = 17.0;
   
   a = (float)(b);
   
   output(a);
   
   b = 18.9;
   a = (int) b;
   
   output(a);

}
